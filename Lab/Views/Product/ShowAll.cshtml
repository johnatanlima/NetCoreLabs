@model List<Lab.Models.Product>

<html>
    <body>
        <h1> @ViewData["Heading"] </h1>
        <table border="1">
            <tr>
                <td> ID </td>
                <td> Name </td>
                <td> Price </td>
            </tr>

            @foreach(var p in Model){
                <tr>
                    <td> @p.Id </td>
                    <td> @p.Name </td>
                    <td> @p.Price </td>
                </tr>
            }

        </table>
    </body>
</html>